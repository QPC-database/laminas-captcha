{
    "name": "laminas/laminas-captcha",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "captcha"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-captcha/",
        "issues": "https://github.com/laminas/laminas-captcha/issues",
        "source": "https://github.com/laminas/laminas-captcha",
        "rss": "https://github.com/laminas/laminas-captcha/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev",
            "dev-develop": "2.4-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-math": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-recaptcha": "*",
        "laminas/laminas-session": "self.version",
        "laminas/laminas-text": "self.version",
        "laminas/laminas-validator": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-recaptcha": "Laminas\\ReCaptcha component",
        "laminas/laminas-resources": "Translations of captcha messages",
        "laminas/laminas-session": "Laminas\\Session component",
        "laminas/laminas-text": "Laminas\\Text component",
        "laminas/laminas-validator": "Laminas\\Validator component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Captcha\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-captcha": "self.version"
    }
}
